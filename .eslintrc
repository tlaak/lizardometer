{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "semi": [2, "always"],
    "max-len": [2, 120, 2],
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "space-before-function-paren": [2, "never"]
  }
}
